{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run redirects:enable && remix build",
    "dev": "npm run redirects:disable && remix dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects"
  },
  "dependencies": {
    "@netlify/functions": "^2.6.3",
    "@netlify/remix-adapter": "^2.6.0",
    "@remix-run/react": "^2.9.1",
    "@remix-run/vercel": "^1.19.3",
    "@storyblok/react": "^3.0.10",
    "@tailwindcss/typography": "^0.5.12",
    "cross-env": "^7.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.1",
    "@remix-run/eslint-config": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "@types/estree": "^1.0.6",
    "@types/estree-jsx": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.13.0",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.27.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  }
}
