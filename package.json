{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run redirects:disable && remix build",
    "dev": "npm run redirects:disable && remix dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects",
    "proxy": "mkcert -install && mkcert localhost && local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem"
  },
  "dependencies": {
    "@netlify/functions": "^2.6.3",
    "@netlify/remix-adapter": "^2.6.0",
    "@remix-run/react": "^2.9.1",
    "@storyblok/react": "^3.0.10",
    "@tailwindcss/typography": "^0.5.12",
    "cross-env": "^7.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shx": "^0.3.4",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.1",
    "@remix-run/eslint-config": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.27.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  }
}
